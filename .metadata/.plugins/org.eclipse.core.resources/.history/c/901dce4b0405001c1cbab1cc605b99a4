		System.out.println("Mario ganhou 1000 pontos");
		return this;
	}

	@Override
	public MarioState pegarFlor() {
		System.out.println("Mario com fogo");
		return MarioFogo;
	}


	@Override
	public MarioState levarDano() {
		System.out.println("Mario pequeno");
		return null;
	}

}
